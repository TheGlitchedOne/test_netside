    id INTEGER PRIMARY KEY AUTOINCREMENT,

    -- Identifikasjon
    session_id TEXT,           -- unik session ID
    ip TEXT,                   -- IP-adresse

    -- Browser + OS
    user_agent TEXT,           -- full user-agent-streng
    browser TEXT,              -- f.eks. Chrome, Firefox
    operating_system TEXT,     -- Windows, Android, iOS osv.

    -- Geolokasjon
    country TEXT,              -- Land
    city TEXT,                 -- By
    latitude REAL,             -- Valgfritt: breddegrad
    longitude REAL,            -- Valgfritt: lengdegrad

    -- Brukertype
    user_type TEXT DEFAULT 'guest',  -- gjest / registrert / admin osv.

    -- Melding eller navn
    name TEXT,                 -- frivillig navn fra bruker

    -- Automatisk tidsstempel
    timestamp DATETIME DEFAULT CURRENT_TIMESTAMP
);
